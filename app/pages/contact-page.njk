	<link rel="stylesheet" href="css/contact-page.min.css">
</head>

<body>
	{% include 'app/html/_header.html' %}
	<main class="main">
	{% include 'app/html/top/_top-contact.html' %}
	{% include 'app/html/_contact-page.html' %}
	{% include 'app/html/_insights-clients.html' %}
	</main>

	{% include 'app/html/_footer.html' %}

	<script src="https://maps.googleapis.com/maps/api/js?key=&callback=initMap&libraries=&v=weekly" defer></script>
	<script>
		let map;

		function initMap() {

			const location = {
				lat: 29.760751697998042,
				lng: -95.36048876547676
			};
			const locationMarker = {
				lat: 29.758792467167368,
				lng: -95.28268445928248
			};
			map = new google.maps.Map(document.getElementById("map"), {
				center: location,
				zoom: 9,
			});
			const image = 'images/icons/location-map.svg';
			const marker = new google.maps.Marker({
				position: locationMarker,
				map: map,
				icon: image,
			});
		}
	</script>
	<script src="js/main.min.js"></script>
</body>

